<form method="post" action="{{ path('dashboard_admin_product_shipping_delete', {'id': shipping.id}) }}" onsubmit="return confirm('{{ "Are you sure you want to delete this item?"|trans }}');">
    <input type="hidden" name="_token" value="{{ csrf_token('shipping_deletion_' ~ shipping.id) }}">
    <button class="btn btn-danger btn-sm">
        <i class="fas fa-trash fa-fw"></i>
    </button>
</form>
