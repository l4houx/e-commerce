{% if app.request.attributes.get('_route') == 'dashboard_account_ticket_index' %}
	{% for ticket in tickets %}
		{{ component('ticket', { 'ticket': ticket, 'hidden': true, 'loop': loop }) }}
	{% else %}
	{% component alert with {type: 'info', closeButton: false, class: 'my-2'} %}
		{% block content %}
			{{ 'No tickets found'|trans }}
		{% endblock %}
	{% endcomponent %}
	{% endfor %}
{% else %}
	{{ component('ticket', { 'ticket': ticket }) }}
{% endif %}
